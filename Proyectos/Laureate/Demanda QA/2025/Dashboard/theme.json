{
  "name": "LIU QA Dashboard (Extendido v3)",
  "dataColors": ["#2F80ED","#F2994A","#27AE60","#9B51E0","#56CCF2","#EB5757","#F2C94C","#4F4F4F"],
  "background": "#FFFFFF",
  "foreground": "#333333",
  "tableAccent": "#2F80ED",
  "visualStyles": {
    "*": {
      "*": {
        "title": { "show": true, "fontSize": 12 },
        "labels": { "fontSize": 10 },
        "legend": { "show": true, "fontSize": 10 },
        "border": { "show": false },
        "background": { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 0 }
      }
    },
    "card": {
      "*": {
        "title": { "show": true },
        "labels": { "fontSize": 18 },
        "categoryLabels": { "fontSize": 10 }
      }
    },
    "slicer": {
      "*": {
        "title": { "show": true },
        "background": { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 0 }
      }
    }
  }
}
