            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Guardar el token extra√≠do por el JSON Extractor en una Property global
def tok = vars.get(&apos;accessToken&apos;)
if (tok) {
    props.put(&apos;accessToken&apos;, tok)
    log.info(&apos;Token guardado en props. len=&apos; + tok.length())
} else {
    log.error(&apos;No se extrajo accessToken. Body=&apos; + prev.getResponseDataAsString())
}
</stringProp>
